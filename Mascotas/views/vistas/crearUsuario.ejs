<div style='background-color:white;color:black;'>

  <h1>Crea Usuarios</h1>

  <form action="Usuario/CrearUsuarioForm" method="post">
    <div class="form-group">
      <label>Nombres</label>
      <input minlength=4 required type="text" class="form-control"  placeholder="Nombres" name="nombres">
    </div>
    <div class="form-group">
      <label>Apellidos</label>
      <input minlength=4 required type="text" class="form-control"  placeholder="Apellidos" name="apellidos">
    </div>
    <div class="form-group">
      <label>Correo</label>
      <input type="email" class="form-control"  placeholder="Correo" name="correo">
    </div>
    <button type="submit" class="btn btn-success">Crear Usuario</button>
    <br>
  </form>
</div>
